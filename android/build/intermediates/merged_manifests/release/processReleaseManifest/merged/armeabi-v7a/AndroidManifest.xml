    <uses-permission android:name="com.google.android.c2dm.permission.RECEIVE" />

    <uses-feature
        android:glEsVersion="0x00020000"
        android:required="true" />

    <permission
        android:name="com.inoek.apps.games.ballbounzreloaded.permission.C2D_MESSAGE"
        android:protectionLevel="signature" />

    <uses-permission android:name="com.inoek.apps.games.ballbounzreloaded.permission.C2D_MESSAGE" />

    <application
